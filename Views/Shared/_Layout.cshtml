<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewData["Title"] - Inmobiliaria</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css">
</head>
<body>
    @* <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
    *@
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark navbar-toggler)">
        <div class="container-fluid">
            <a class="navbar-brand" asp-controller="Home" asp-action="Index">Inmobiliaria</a>
            <div>
                <ul class="navbar-nav">
                    <li class="nav-item">
                        <a class="nav-link" asp-controller="Home" asp-action="Index">Home</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-controller="Propietarios" asp-action="Index">Propietarios</a>
                    </li>
                    <li class="nav-item"></li>
                        <a class="nav-link" asp-controller="Inquilinos" asp-action="Index">Inquilinos</a>
                    </li>
                    <li class="nav-item"></li>
                        <a class="nav-link" asp-controller="Inmuebles" asp-action="Index">Inmuebles</a>
                    </li>
                     @* <li class="nav-item"></li>
                        <a class="nav-link" asp-controller="Usuarios" asp-action="Index">Usuarios</a>
                    </li> *@
                    <li class="nav-item"></li>
                        <a class="nav-link" asp-controller="Contratos" asp-action="Index">Contratos</a>
                    </li>


                </ul>
            </div>
        </div>
    </nav>

    <div class="container mt-3">
        @RenderBody()
    </div>
    @RenderSection("Scripts", required: false)
</body>
</html>