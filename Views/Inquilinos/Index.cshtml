@model IEnumerable<Inmobiliaria.Models.Inquilino>

<h2>Listado de Inquilinos</h2>
<a asp-action="Create">Nuevo Inquilino</a>
<table border="1" cellpadding="5">
    <tr>
        <th>DNI</th>
        <th>Apellido</th>
        <th>Nombre</th>
        <th>Email</th>
        <th>Tel√©fono</th>
        <th>Domicilio</th>
        <th>Acciones</th>
    </tr>
    @foreach (var p in Model)
    {
        <tr>
            <td>@p.dniInquilino</td>
            <td>@p.apellido</td>
            <td>@p.nombre</td>
            <td>@p.email</td>
            <td>@p.telefono</td>
            <td>@p.domicilioPersonal</td>
            <td>
            <a asp-action="Edit" asp-route-id="@p.idInquilino">Editar</a> |
            <a asp-action="Delete" asp-route-id="@p.idInquilino">Eliminar</a>
            </td>
        </tr>
    }
</table>